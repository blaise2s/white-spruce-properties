{
  "name": "white-spruce-properties",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react-dom": "^17.0.0",
    "express": "^4.17.1",
    "normalize.css": "^8.0.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "typescript": "^4.0.3",
    "web-vitals": "^0.2.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/express": "^4.17.11",
    "@types/react-router-dom": "^5.1.7",
    "husky": "^4.3.8",
    "node-sass": "4.14.1",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "typed-scss-modules": "^3.4.1"
  },
  "scripts": {
    "build": "yarn tsm && react-scripts build && yarn build-app-server",
    "build-app-server": "tsc app-server.ts --outDir build/",
    "dev": "yarn tsm && react-scripts start",
    "eject": "react-scripts eject",
    "prettier": "pretty-quick",
    "start": "node build/app-server.js",
    "test": "react-scripts test",
    "tsm": "rm -f 'src/**/*.scss.d.ts' && tsm 'src/**/*.scss'"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
